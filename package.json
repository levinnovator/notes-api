{
  "name": "notes-api",
  "version": "0.1.0",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "start:prod": "cross-env NODE_ENV=production yarn start",
    "start:dev": "nodemon",
    "build": "rimraf build && babel src --extensions .ts --out-dir build --source-maps inline",
    "lint": "concurrently \"yarn lint:js\" \"yarn lint:types\"",
    "lint:js": "eslint --ext .ts src",
    "lint:types": "tsc -p .",
    "prettier": "prettier --write src"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2"
  }
}
